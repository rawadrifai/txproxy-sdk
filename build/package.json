  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rawadrifai/txproxy-sdk.git"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "bitcoin",
    "ripple",
    "transaction",
    "proxy"
  ],
  "author": "Rawad Rifai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rawadrifai/txproxy-sdk/issues"
  },
  "homepage": "https://github.com/rawadrifai/txproxy-sdk#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.2.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.23.0",
    "braintree-jsdoc-template": "^3.3.0",
    "husky": "^1.0.0-rc.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true
  }
