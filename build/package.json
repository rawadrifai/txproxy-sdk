{
  "name": "txproxy-sdk",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./build && rm -rf ./docs/build",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir ./build --ignore test.js",
    "postbuild": "cp ./package.json ./build",
    "test": "jest",
    "pub": "yarn run build; yarn publish ./build",
